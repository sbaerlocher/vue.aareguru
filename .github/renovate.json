{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>sbaerlocher/sbaerlocher:renovate-js"],
  "packageRules": [
    {
      "groupName": "Vue",
      "matchPackageNames": ["/^vue/", "/^@vue/"]
    },
    {
      "groupName": "Vite",
      "matchPackageNames": ["/^vite$/", "/^@vitejs/"]
    }
  ]
}
