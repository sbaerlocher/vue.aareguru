{
  "name": "vue.aareguru",
  "version": "2.3.0",
  "type": "module",
  "description": "Vue 3 component that displays the Aare river temperature from Aareguru API.",
  "main": "dist/vue.aareguru.cjs",
  "module": "dist/vue.aareguru.mjs",
  "unpkg": "dist/vue.aareguru.js",
  "jsdelivr": "dist/vue.aareguru.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "vite build && npm run build:types",
    "build:bundle": "npm run build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "format": "prettier --write '**/*.{vue,ts,js,json,md,css,yml,yaml}'",
    "format:check": "prettier --check '**/*.{vue,ts,js,json,md,css,yml,yaml}'",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "validate": "npm run format:check && npm run type-check && npm run lint && npm run test",
    "clean": "rm -rf dist coverage node_modules/.vite",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "prepublishOnly": "npm run type-check && npm run test:coverage",
    "storybook": "npm run dev",
    "build:storybook": "storybook build"
  },
  "files": [
    "dist",
    "src/components/AareGuru.vue",
    "src/types/index.d.ts"
  ],
  "types": "dist/types/src/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/vue.aareguru.mjs",
      "require": "./dist/vue.aareguru.cjs"
    },
    "./types": {
      "types": "./src/types/index.d.ts"
    },
    "./style.css": "./dist/vue.aareguru.css",
    "./package.json": "./package.json"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "aare",
    "aareguru",
    "temperature",
    "switzerland",
    "bern",
    "river"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbaerlocher/vue.aareguru.git"
  },
  "bugs": {
    "url": "https://github.com/sbaerlocher/vue.aareguru/issues"
  },
  "homepage": "https://github.com/sbaerlocher/vue.aareguru#readme",
  "peerDependencies": {
    "axios": "^1.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@storybook/addon-a11y": "^10.2.10",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/addon-onboarding": "^10.2.10",
    "@storybook/addon-vitest": "^10.2.10",
    "@storybook/vue3-vite": "^10.2.10",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/compiler-sfc": "^3.5.28",
    "@vue/test-utils": "^2.4.6",
    "axios": "^1.13.5",
    "eslint": "^10.0.1",
    "eslint-plugin-storybook": "^10.2.10",
    "eslint-plugin-vue": "^10.8.0",
    "happy-dom": "^20.6.5",
    "msw": "^2.12.10",
    "msw-storybook-addon": "^2.0.6",
    "playwright": "^1.58.2",
    "prettier": "^3.5.3",
    "storybook": "^10.2.10",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue": "^3.5.28",
    "vue-eslint-parser": "^10.4.0",
    "vue-tsc": "^3.2.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "overrides": {
    "glob": "^13.0.0"
  }
}
